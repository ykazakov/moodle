  \fi
  \ifnum\c@moodle@numcorrectanswers=0\relax%
    \PackageError{moodle}{No answer is explicitly marked as correct (*). Be sure one answer leads to points.}%
  \fi
  \ifnum\c@moodle@numcorrectanswers>1\relax%
