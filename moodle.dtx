\def\moodle@makelatextag@multi{%
  \ifmoodle@multiple
    \moodle@makelatextagbox{Multiple}\relax%
  \else
    \moodle@makelatextagbox{Single}\relax%
  \fi
  \ifmoodle@handout\else
    \ifmoodle@shuffle
      \moodle@makelatextagbox{Shuffle}\relax%
    \fi
  \fi
