%  \fi
  \setcounter{moodle@NumStarredAnswers}{0}%
  \begin{itemize} \setlength\itemsep{0pt}\setlength\parskip{0pt}%
    \loopthroughitemswithcommand{\moodle@print@truefalse@answer}%
    \ifnum\c@currentitemnumber=2\relax
